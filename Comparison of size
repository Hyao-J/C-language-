#include<stdio.h>
int main()
{
	float a, b;     //浮点型，可使用整型int使用
	scanf_s("%f%f", &a, &b);
	if (a > b)
		printf("%f>%f", a, b);
	else
		if (a = b)
			printf("%f=%f", a , b);
	else
		printf("%f<%f", a, b);
	}
	



#include<stdio.h>
int max(int i, int j);  //声明i,j
int main()
{
	int i,  j;       
	printf("输入两个数：");
	scanf_s("%d%d", &i, &j);
	printf("%d\n", max(i,j));
	}
	int  max(int i, int j)  //重新声明i,j
	{
		if (i > j)
			return i;
		else
			return j;
	}
  
